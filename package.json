  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "packageManager": "pnpm@10.6.1",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@vercel/speed-insights",
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "@vercel/speed-insights",
      "esbuild"
    ]
  }
